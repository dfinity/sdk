{
  "compilerOptions": {
    "incremental": true,
    "target": "es2017",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2017"
    ],
    "declaration": true,
    "sourceMap": true,
    "tsBuildInfoFile": "./build_info.json",
    "strict": true,
    "paths": {
        "@dfinity/agent": [
            "../agent/javascript",
            "node_modules/@dfinity/agent"
        ]
    },
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
