{
  "version": 1,
  "dfx": "{dfx_version}",
  "canisters": {
    "{project_name}": {
      "type": "rust",
      "package": "{project_name}",
      "candid": "src/{project_name}/{project_name}.did"
    },
    "{project_name}_frontend": {
      "type": "assets",
      "source": [
        "src/{project_name}_frontend/assets"
      ],
      "dependencies": [
        "{project_name}"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
