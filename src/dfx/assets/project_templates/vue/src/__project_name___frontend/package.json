{
  "name": "{project_name}_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && dfx canister create {project_name}_backend && dfx generate {project_name}_backend && dfx deploy",
    "start": "vite --port 3000",
