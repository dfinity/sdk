    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "dotenv": "^16.3.1",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3"
  },
  "dependencies": {
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "@dfinity/agent": "^0.20.0",
    "@dfinity/candid": "^0.20.0",
    "@dfinity/principal": "^0.20.0"
  }
