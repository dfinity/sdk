#!/usr/bin/expect -df

match_max 100000
set timeout 5

spawn dfx cache install
expect "installed successfully."

spawn dfx start --clean
set pid [exp_pid]
expect "Using" {
    sleep 1
    send \x03
}

expect {
    timeout {
        puts "Didn't complete before timeout."
        exec kill -9 $pid
        exit 2
    }
}
