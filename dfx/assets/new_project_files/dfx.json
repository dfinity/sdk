{
  "version": 1,
  "dfx": "{dfx_version}",
  "canisters": {
    "{project_name}": {
      "main": "src/{project_name}/main.mo"
    }
  },
  "defaults": {
    "build": {
      "output": "canisters/"
    },
    "start": {
      "port": 8000,
      "address": "127.0.0.1",
      "serve_root": "canisters/{project_name}/assets"
    }
  }
}
